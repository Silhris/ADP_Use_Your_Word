alias st='git status'
alias a='!git add . && git status'
alias au='!git add -u . && git status'
alias aa='!git add . && git add -u . && git status'
alias c='git commit'
alias cm='git commit -m'
alias ca='git commit --amend'
alias ac='!git add . && git commit'
alias acm='!git add . && git commit -m'
alias l='git log --graph --all --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset''
alias ll='git log --stat --abbrev-commit'
alias lg='git log --color --graph --pretty=format:'%C(bold white)%h%Creset -%C(bold green)%d%Creset %s %C(bold green)(%cr)%Creset %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative'
alias llg='git log --color --graph --pretty=format:'%C(bold white)%H %d%Creset%n%s%n%+b%C(bold blue)%an <%ae>%Creset %C(bold green)%cr (%ci)' --abbrev-commit'
alias d='git diff'
alias co='git checkout'
alias master='git checkout master'
alias alias='!git config --list | grep 'alias\.' | sed 's/alias\.\([^=]*\)=\(.*\)/\1\	 => \2/' | sort'
alias alia-edit='git config --global -e'
alias ane='git commit --amend --no-edit'
alias bl='git branch --list'
alias blr='git branch -r'
alias chkd='git checkout develop'
alias feature-log='git log develop..$(git rev-parse --abbrev-ref HEAD)'
alias fo='git fetch origin'
alias last='git show HEAD'
alias las-name-only='git show HEAD --name-only'
alias oups='git commit --amend'
alias pod='git pull origin develop'
alias trash='git reset --hard HEAD'
alias whichbranchiam='git rev-parse --abbrev-ref HEAD'
alias wtf='git commit -m $(curl -s http://whatthecommit.com/index.txt )'
alias tag-refreshgit='git tag -l | xargs git tag -d && git fetch -t'